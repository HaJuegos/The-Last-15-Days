{
	"format_version": "1.17.30",
	"animation_controllers": {
		"controller.animation.ha.ejecuted_scape_piglin": {
			"states": {
				"default": {
					"transitions": [
						{
							"scape_event": "query.health <= 20"
						}
					]
				},
				"scape_event": {
					"transitions": [
						{
							"default": "!query.health <= 20"
						}
					],
					"on_entry": [
						"/execute @s[tag=!regeneracion] ~ ~ ~ event entity @s become_calm_event",
						"/tag @s[tag=!regeneracion] add regeneracion"
					]
				}
			}
		}
	}
}